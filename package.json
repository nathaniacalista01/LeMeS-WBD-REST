{
  "name": "tugas-besar-2-rest",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "bcrypt": "^5.1.1",
    "bcrypt-ts": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "typescript": "^5.2.2",
    "xml-js": "^1.6.11"
  },
  "scripts": {
    "build": "yarn tsc",
    "start": "node dist/index.js",
    "seed": "yarn prisma db seed",
    "dev": "concurrently \"yarn tsc --watch\" \"nodemon -q dist/index.js\"",
    "start:prod": "yarn migrate:dev && yarn seed && concurrently \"yarn tsc --watch\" \"nodemon -q dist/index.js\" ",
    "migrate:dev": "yarn prisma migrate dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1"
  }
}
